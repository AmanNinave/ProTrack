{ 
    "version": 2, 
    "builds" : [ {"src" : "./index.js" , "use" : "@vercel/node" }],
    "routes" : [ 
        { 
            "src" : "./(.*)" , 
            "dest" : "./" ,
            "methods" : ["GET" , "POST" , "PUT" , "DELETE" , "PATCH" , "UPDATE" , "OPTIONS" ],
            "allowedHeaders" : ["Content-Type", "Authorization"],
            "origin": "*",
            "headers" : {
                "Access-Control-Allow-Origin" : "*"
            }
        }
    ]
    
}